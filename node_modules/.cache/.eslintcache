h';\nimport { AppCtx } from '../AppContext';\nimport { IAppContextInterface } from '../AppContext';\n\nconst Characters: FC = () => {\n  const [characters, setCharacters] = useState<ICharacter[]>([]);\n  const [info, setInfo] = useState<IPaginationInfo>();\n  const [character, setDetailInfo] = useState<ICharacter | null>(null);\n  const [error, setError] = useState<string>('');\n  const [active, setActive] = useState<number>(1);\n\n  function getPage(url: string) {\n    fetch(url)\n      .then((response) => response.json())\n      .then((data) => {\n        if (data?.error) {\n          setError(data.error);\n        } else {\n          setError('');\n        }\n        setCharacters(data.results);\n        setInfo(data.info);\n      });\n  }\n\n  useEffect(() => {\n    getPage('https://rickandmortyapi.com/api/character');\n  }, []);\n\n  // const {\n  //   characters,\n  //   setCharacters,\n  //   info,\n  //   setInfo,\n  //   character,\n  //   setDetailInfo,\n  //   error,\n  //   setError,\n  //   active,\n  //   setActive,\n  //   getPage,\n  // } = useContext<IAppContextInterface>(AppCtx);\n\n  return (\n    <>\n      <Search getPage={getPage} />\n      <Pagination\n        info={info}\n        getPage={getPage}\n        active={active}\n        setActive={setActive}\n      />\n      {error && <div>{error}</div>}\n      <CharactersList setDetailInfo={setDetailInfo} characters={characters} />\n      {character && (\n        <DetailPage character={character} setDetailInfo={setDetailInfo} />\n      )}\n      <Pagination\n        info={info}\n        getPage={getPage}\n        active={active}\n        setActive={setActive}\n      />\n    </>\n  );\n};\n\nexport default Characters;\n","/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/Pagination.tsx",["60"],[],"import React, { FC, useState } from 'react';\nimport { IPaginationInfo } from '../types/types';\nimport './styles/pagination.scss';\n\ninterface Info {\n  info: IPaginationInfo | undefined;\n  getPage: (url: string) => void;\n  active: number;\n  setActive: (num: number) => void;\n}\n\nconst allPages = (\n  start: number,\n  end: number,\n  pageType: string,\n  url: string\n) => {\n  let res = [];\n  for (let i = start, j = 1; i <= end; i += 1, j += 1) {\n    const newUrl = url\n      .replace(/\\d+/g, String(i))\n      .replace(/\\/\\w+\\/\\?/, `/${pageType}?`);\n\n    res.push({\n      url: newUrl,\n      num: i,\n    });\n  }\n  return res;\n};\n\nconst Pagination: FC<Info> = ({ info, getPage, active, setActive }) => {\n  let all;\n  if (info) {\n    let start;\n    let end;\n\n    if (window.innerWidth < 768) {\n      start = active - 3 > 0 && info.pages > 20 ? active - 2 : 1;\n      end = active + 3 < info.pages ? active + 3 : info.pages;\n    } else {\n      start = active - 10 > 0 && info.pages > 20 ? active - 10 : 1;\n      end = active + 10 < info.pages ? active + 10 : info.pages;\n    }\n\n    let url;\n    if (typeof info.next === 'string') {\n      url = String(info.next);\n    } else {\n      url = String(info.prev);\n    }\n    all = allPages(start, end, 'character', url);\n  }\n\n  const onClickHandler = (url: string, num: number) => {\n    getPage(url);\n    setActive(num);\n  };\n\n  return (\n    <div className=\"container\">\n      {all?.map((el) => (\n        <div\n          className={active === el.num ? 'active' : undefined}\n          key={el.num}\n          onClick={() => onClickHandler(el.url, el.num)}\n        >\n          {el.num}\n        </div>\n      ))}\n    </div>\n  );\n};\n\nexport default Pagination;\n","/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/Search.tsx",["61","62"],[],"import React, { FC, useState, ChangeEventHandler, useEffect } from 'react';\nimport './styles/search.scss';\n\ninterface Search {\n  getPage: (url: string) => void;\n}\n\nconst Search: FC<Search> = ({ getPage }) => {\n  const [input, setInput] = useState<string>('');\n\n  const onChange: ChangeEventHandler<HTMLInputElement> = (e) => {\n    setInput(e.target.value);\n    setTimeout(() => {\n      if (input && getPage) {\n        getPage(`https://rickandmortyapi.com/api/character/?name=${input}`);\n      }\n    }, 2000);\n  };\n\n  return (\n    <div className=\"search\">\n      <input\n        value={input}\n        onChange={onChange}\n        type=\"text\"\n        placeholder=\"CHARACTER`S NAME\"\n      />\n    </div>\n  );\n};\n\nexport default Search;\n","/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/Characters/CharactersList.tsx",[],[],"/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/Characters/DetailPage.tsx",["63"],[],"import React, { FC, useState } from 'react';\nimport { CharacterCardProps } from './CharactersCard';\n\nconst DetailPage: FC<CharacterCardProps> = ({ character, setDetailInfo }) => {\n  const [hide, setHide] = useState<string>('detail-window');\n\n  const onClickHandler = () => {\n    setHide('detail-window hide');\n    setTimeout(() => {\n      setDetailInfo(null);\n    }, 500);\n  };\n\n  return (\n    <div className=\"detail-container\">\n      <div className={hide}>\n        <div>\n          <img src={character.image} />\n        </div>\n\n        <div className=\"info\">\n          <div>\n            <strong>Name: </strong>\n            {character.name}\n          </div>\n          <div>\n            <strong>Gender: </strong>\n            {character.gender}\n          </div>\n          <div>\n            <strong>Status: </strong> {character.status}\n          </div>\n          <div>\n            <strong>Species: </strong>Species: {character.species}\n          </div>\n          <div>\n            <strong>Location: </strong>\n            {character.location.name}\n          </div>\n          <div>\n            <strong>Origin: </strong>\n            {character.origin.name}\n          </div>\n        </div>\n\n        <div className=\"episodes\">\n          <div>Episodes:</div>{' '}\n          <ul>\n            {character.episode.map((el) => (\n              <li key={el}>{el}</li>\n            ))}\n          </ul>\n        </div>\n        <button onClick={onClickHandler}>Back</button>\n      </div>\n    </div>\n  );\n};\nexport default DetailPage;\n","/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/Characters/CharactersCard.tsx",[],[],"/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/AppContext.tsx",[],[],"/Users/aleksander/Documents/portfolio/rick-and-morty/src/components/NavBar.tsx",[],[],{"ruleId":"64","severity":1,"message":"65","line":1,"column":21,"nodeType":"66","messageId":"67","endLine":1,"endColumn":31},{"ruleId":"64","severity":1,"message":"68","line":8,"column":10,"nodeType":"66","messageId":"67","endLine":8,"endColumn":16},{"ruleId":"64","severity":1,"message":"69","line":9,"column":10,"nodeType":"66","messageId":"67","endLine":9,"endColumn":30},{"ruleId":"64","severity":1,"message":"70","line":1,"column":21,"nodeType":"66","messageId":"67","endLine":1,"endColumn":29},{"ruleId":"64","severity":1,"message":"71","line":1,"column":51,"nodeType":"66","messageId":"67","endLine":1,"endColumn":60},{"ruleId":"72","severity":1,"message":"73","line":8,"column":7,"nodeType":"66","messageId":"74","endLine":8,"endColumn":25},{"ruleId":"75","severity":1,"message":"76","line":18,"column":11,"nodeType":"77","endLine":18,"endColumn":40},"@typescript-eslint/no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","'AppCtx' is defined but never used.","'IAppContextInterface' is defined but never used.","'useState' is defined but never used.","'useEffect' is defined but never used.","@typescript-eslint/no-redeclare","'Search' is already defined.","redeclared","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]